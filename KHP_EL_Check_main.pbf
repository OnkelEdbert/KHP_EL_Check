Global Canvas_3D_View, Button_Load_STL, Button_Load_IGS

Enumeration FormWindow
  #KHP_EL_Check_Main_Window
EndEnumeration

Declare Button_Load_IGS(EventType)
Declare Button_Load_STL(EventType)

Procedure OpenKHP_EL_Check_Main_Window(x = 0, y = 0, width = 1670, height = 960)
  OpenWindow(#KHP_EL_Check_Main_Window, x, y, width, height, "", #PB_Window_SystemMenu | #PB_Window_MinimizeGadget)
  Canvas_3D_View = CanvasGadget(#PB_Any, 10, 10, 1500, 880)
  Button_Load_STL = ButtonGadget(#PB_Any, 1550, 40, 80, 20, "Load STL")
  Button_Load_IGS = ButtonGadget(#PB_Any, 1550, 80, 80, 20, "Load IGS")
EndProcedure

Procedure KHP_EL_Check_Main_Window_Events(event)
  Select event
    Case #PB_Event_CloseWindow
      ProcedureReturn #False

    Case #PB_Event_Menu
      Select EventMenu()
      EndSelect

    Case #PB_Event_Gadget
      Select EventGadget()
        Case Button_Load_STL
          Button_Load_STL(EventType())          
        Case Button_Load_IGS
          Button_Load_IGS(EventType())          
      EndSelect
  EndSelect
  ProcedureReturn #True
EndProcedure

